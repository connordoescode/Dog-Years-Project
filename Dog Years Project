const myAge = 28;
var earlyYears = 2;
earlyYears *= 10.5;
let laterYears = myAge - 2;
laterYears *= 4;
var myName = "Connor";
var myAgeInDogYears = "28";
console.log(
  `my name is ${myName}. I am ${myAgeInDogYears} years old in dog years.`
);
myAgeInDogYears = "earlyYears" + "laterYears";
//variables named myAgeInDogYears.//
